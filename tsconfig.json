{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false
  },
  "include": [
    "app/**/*.ts",
    "app/**/*.tsx",
    "prompts/**/*.ts"
  ],
  "exclude": [
    "node_modules",
    "build",
    "dist",
    ".expo",
    "supabase"
  ]
}
